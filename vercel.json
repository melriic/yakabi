{
  "redirects": [
    {
      "source": "https://www.yakabi.app/:path*",
      "destination": "https://yakabi.app/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(cgu|privacy|rules|contact)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
